{
  "name": "react-chello",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.14.3",
    "@googlemaps/react-wrapper": "^1.1.34",
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@ramonak/react-progress-bar": "^5.0.0",
    "@react-google-maps/api": "^2.12.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.0.1",

    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^9.8.3",
    "firebaseui": "^6.0.1",
    "moment": "^2.29.3",
    "quill": "^1.3.7",
    "react": "^18.1.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-big-calendar": "^0.40.2",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.1",
    "react-firebaseui": "^6.0.0",
    "react-loading": "^2.0.3",
    "react-mentions": "^4.4.2",
    "react-quilljs": "^1.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.3.2",
    "react-toastify": "^9.0.4",
    "react-uuid": "^1.0.2",
    "uuidv4": "^5.0.1",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "main": "dist/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"react-scripts start\" \"npm run electron\"" ,
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=dist/main.js",
    "electron": "wait-on tcp:3000 && electron .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron-cra",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_sini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron-builder": "^23.1.0",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "electron": "^19.0.4",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.2",
    "wait-on": "^6.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "react_chello"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
